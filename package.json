{
  "name": "frontend-interview-takehome",
  "version": "1.0.0",
  "description": "Mock JSON server to support Frontend take-home interview.",
  "author": "GOAT Engineering <dev@goat.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "10.15.0"
  },
  "engineStrict": true,
  "scripts": {
    "start:json": "json-server api.json --host 0.0.0.0 --port 5000 --watch",
    "start": "node tool/bin/react-scripts.js start",
    "build": "node tool/bin/react-scripts.js build",
    "start:static": "node tool/quick-server.js 3000",
    "test:init": "webdriver-manager update",
    "test": "protractor e2e/conf.js",
    "eject": "react-scripts eject",
    "build:tool": "cd ts && tsc -w",
    "build:ssr": "node tool/scripts/ssr.js && cd ts && tsc -p tsconfig.iso.json -w",
    "check-types": "tsc",
    "analyze": "webpack-bundle-analyzer build/stats.json",
    "lint": "tslint -c tslint.json {src,ts}/**/**/*.t{sx,s}",
    "lint:fix": "tslint --fix -c tslint.json {src,ts}/**/**/*.t{sx,s}"
  },
  "dependencies": {
    "json-server": "^0.14.2",
    "node-sass": "^4.13.1",
    "require-injector": "~5.1.0",
    "source-map-support": "~0.5.12",
    "fs-extra": "~7.0.1",
    "@types/fs-extra": "~7.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/node": "12.0.0",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "typescript": "3.4.5",
    "node-sass": "~4.13.1",
    "cross-fetch": "~3.0.3",
    "lodash": "~4.17.11",
    "log4js": "~4.3.0",
    "typeface-roboto": "~0.0.54",
    "classnames": "~2.2.6",
    "@types/classnames": "~2.2.7",
    "@material-ui/core": "4.0.0",
    "@material-ui/icons": "4.0.0",
    "react-router-dom": "~5.0.0",
    "@types/react-router-dom": "~4.3.3",
    "@loadable/component": "~5.10.1",
    "@types/loadable__component": "~5.9.0",
    "matchmedia-polyfill": "^0.3.2",
    "rxjs": "~6.5.2",
    "@types/webpack": "~4.4.32",
    "protractor": "^5.4.2",
    "express": "~4.17.1",
    "@types/express": "~4.16.1",
    "compression": "^1.7.4",
    "@types/serve-index": "^1.7.29",
    "serve-index": "^1.9.1",
    "ts-node": "~8.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "tslint-eslint-rules": "^4.1.1",
    "tslint": "^5.17.0",
    "tslib": "^1.9.3",
    "tslint-react-hooks": "^2.1.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "stats-webpack-plugin": "^0.7.0"
  },
  "browserslist": {
    "production": [
      "ie >= 8",
      "ff >= 30",
      "chrome >= 34",
      "safari >= 7",
      "ios >= 7",
      "android >= 4.0"
    ],
    "development": [
      "ie >= 8",
      "ff >= 30",
      "chrome >= 34",
      "safari >= 7",
      "ios >= 7",
      "android >= 4.0"
    ]
  }
